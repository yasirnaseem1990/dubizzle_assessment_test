<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="app.assessmentdubizzle.com" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".AppController"
        android:allowBackup="false"
        android:icon="@drawable/logo_dubizzle"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/logo_dubizzle"
        android:supportsRtl="true"
        android:theme="@style/Theme.AssessmentDubizzle"
        android:usesCleartextTraffic="true"
        tools:replace="android:allowBackup" >
        <activity android:name=".modules.view.activities.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modules.view.activities.MainActivity" />
    </application>

</manifest>